{
	"name": "dualcoding",
	"displayName": "dualcoding",
	"description": "Syncronous coding as a VS code Plugin",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.52.0"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/StopMakingUsernamesUnique/DualCoding.git"
	},
	"keywords": [
		"Plugin"
	],
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:dualcoding.helloWorld",
		"onView:dualcoding-sidebar"
	],
	"main": "./dist/extension.js",
	"contributes": {
		"viewsContainers": {
			"activitybar": [
				{
					"id": "dualcoding-sidebar-view",
					"title": "DualCoding",
					"icon": "media/icon.svg"
				}
			]
		},
		"views": {
			"dualcoding-sidebar-view": [
				{
					"type": "webview",
					"id": "dualcoding-sidebar",
					"name": "DualCoding",
					"icon": "media/icon.svg",
					"contextualTitle": "DualCoding"
				}
			]
		},
		"commands": [		
			{
				"command": "dualcoding.helloWorld",
				"title": "Hello World"
			}	
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run package",
		"compile": "webpack --config ./build/node-extension.webpack.config.js",
		"watch": "concurrently \"rollup -c -w\" \"webpack --watch --config ./build/node-extension.webpack.config.js\"",
		"package": "webpack --mode production --devtool hidden-source-map --config ./build/node-extension.webpack.config.js",
		"test-compile": "tsc -p ./",
		"test-watch": "tsc -watch -p ./",
		"pretest": "npm run test-compile && npm run lint",
		"lint": "eslint src --ext ts",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@rollup/plugin-commonjs": "^17.0.0",
		"@rollup/plugin-node-resolve": "^11.1.0",
		"@rollup/plugin-typescript": "^8.1.0",
		"@tsconfig/svelte": "^1.0.10",
		"@types/glob": "^7.1.3",
		"@types/mocha": "^8.0.4",
		"@types/node": "^12.11.7",
		"@types/vscode": "^1.52.0",
		"@typescript-eslint/eslint-plugin": "^4.9.0",
		"@typescript-eslint/parser": "^4.9.0",
		"concurrently": "^5.3.0",
		"eslint": "^7.15.0",
		"glob": "^7.1.6",
		"mocha": "^8.1.3",
		"rollup": "^2.30.0",
		"rollup-plugin-svelte": "^6.0.0",
		"rollup-plugin-terser": "^7.0.2",
		"svelte": "^3.32.0",
		"svelte-check": "^1.1.30",
		"svelte-preprocess": "^4.6.3",
		"ts-loader": "^8.0.11",
		"typescript": "^4.1.2",
		"vscode-test": "^1.4.1",
		"webpack": "^5.10.0",
		"webpack-cli": "^4.2.0"
	},
	"author": "Kieran Conroy",
	"license": "ISC",
	"bugs": {
		"url": "https://github.com/StopMakingUsernamesUnique/DualCoding/issues"
	},
	"homepage": "https://github.com/StopMakingUsernamesUnique/DualCoding#readme"
}
